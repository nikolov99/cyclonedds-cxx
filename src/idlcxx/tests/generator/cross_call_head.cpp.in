#include <algorithm>
#include <vector>
#include <cstring>
#include "org/eclipse/cyclonedds/topic/hash.hpp"

template<typename T> size_t write(const A_1::A_2::s_1 &obj, T &stream) {
  (void)obj;
  size_t pos = stream.position();
  stream.write_primitive(obj.m_1());
  return stream.position() - pos;
}

template<typename T> size_t write_size(const A_1::A_2::s_1 &obj, T &stream) {
  (void)obj;
  size_t pos = stream.position();
  stream.incr_primitive(obj.m_1());
  return stream.position() - pos;
}

template<typename T> size_t write_size_max(const A_1::A_2::s_1 &obj, T &stream) {
  (void)obj;
  size_t pos = stream.position();
  stream.incr_primitive(obj.m_1());
  return stream.position() - pos;
}

template<typename T> size_t key_write_size(const A_1::A_2::s_1 &obj, T &stream) {
  (void)obj;
  size_t pos = stream.position();
  return stream.position() - pos;
}

template<typename T> size_t key_write_size_max(const A_1::A_2::s_1 &obj, T &stream) {
